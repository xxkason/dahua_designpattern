<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ShouYinTai"
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="ShouYinTai.MainWindow"
        mc:Ignorable="d" Height="350" Width="525" Title="{DynamicResource TitleString}">
    <Window.Resources>
        <System:String x:Key="TitleString"></System:String>
    </Window.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="64*"/>
            <ColumnDefinition Width="46*"/>
            <ColumnDefinition Width="407*"/>
        </Grid.ColumnDefinitions>
    </Grid>
</Window>
